{
  "name": "hiworldapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.17.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "history": "^4.7.2",
    "node-less-chokidar": "^0.1.2",
    "node-sass-chokidar": "0.0.3",
    "nodemailer": "^4.4.2",
    "nodemon": "^1.14.11",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-form": "^2.16.1",
    "react-input-range": "^1.3.0",
    "react-router-hash-link": "^1.1.1",
    "react-scripts": "1.1.0",
    "react-scrollable-anchor": "^0.6.1",
    "reactstrap": "^5.0.0-beta.2",
    "serve-favicon": "^2.5.0",
    "static-favicon": "^2.0.0-alpha"
  },
  "scripts": {
    "start-server": "babel-node server/app.js",
    "build-css": "node-less-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-less-chokidar src/ -o src/ --watch --recursive src/ -x",
    "start-js": "react-scripts start",
    "start": "babel-node server/app.js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "start-client": "npm-run-all -p watch-css start-js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-object-values-to-object-keys": "^1.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  }
